<h1>Møter</h1>

<% @meetings.each do |meeting| %>
    <div class="row">
        <div class="col-sm-4 col-md-3">
            <div class="thumbnail">
	            <%= link_to image_tag(meeting.img_1), meeting %><br/>
	            <h4><%= link_to meeting.name, meeting %></h4>
	            <%= meeting.meet_date.strftime("%d.%m.%Y") %>
            </div>
        </div>
    </div>
<% end %>

  
<br>

<%= link_to 'Nytt møte', new_meeting_path %>
