<ol class="breadcrumb">
    <li><a href="/">Hjem</a></li>
</ol>
<div class="row">
    <div class="col-sm-10"><h1>Møter</h1></div>
    <div class="col-sm-2">
        <%= link_to 'Nytt møte', new_meeting_path, :class => "btn btn-default btn-xs", :method => :get  %>
    </div>
</div>

<div class="row">
<% @meetings.each do |meeting| %>
        <div class="col-sm-4 col-md-3">
            <div class="thumbnail">
	            <%= link_to image_tag(meeting.img_1), meeting %><br/>
	            <h4><%= link_to meeting.name, meeting %></h4>
	            <%= meeting.meet_date.strftime("%d.%m.%Y") %>
            </div>
        </div>
<% end %>

</div>
  



